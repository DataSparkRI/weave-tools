    <MapTool title="" aspectRatio="1.34" xCenter="" yCenter="" zoomLevel="-0.2" minZoomLevel="" maxZoomLevel="" width="{{ tool.width }}%" height="{{ tool.height }}%">
      {% if tool.primary_geometry_collection %}
      <mapLayer geometryCollection="{{ tool.primary_geometry_collection.name }}" active="true" probe="true" borderOnly="false" borderColor="0x000000" borderThickness="1" opacity="1.0"/>
      {% else %}
      <mapLayer geometryCollection="OICSchools09" active="true" probe="true" borderOnly="false" borderColor="0x000000" borderThickness="1" opacity="1.0"/>
      {% endif %}
      {% if tool.secondary_geometry_collection %}
      <mapLayer geometryCollection="{{ tool.secondary_geometry_collection.name }}" active="true" probe="false" borderOnly="false" borderColor="0x000000" borderThickness="1" opacity="1.0"/>
      {% else %}
      <mapLayer geometryCollection="dst_high" active="true" probe="false" borderOnly="false" borderColor="0x000000" borderThickness="1" opacity="1.0"/>
      {% endif %}
    </MapTool>
